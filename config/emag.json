{
  "startAddress": "https://www.emag.ro/",
  "category": {
    "container": {
      "selector": ".megamenu-item"
    },
    "metadata": {
      "link": {
        "property": "innerText",
        "selector": ""
      }
    }
  },
  "subCategory": {
    "container": {
      "selector": ".emg-aside-links > li > a"
    },
    "metadata": {
      "link": {
        "property": "innerText",
        "selector": ""
      }
    }
  },
  "item": {
    "container": {
      "selector": ".js-card-item"
    },
    "metadata": {
      "price": {
        "property": "innerText",
        "selector": ".product-new-price"
      },
      "name": {
        "property": "innerText",
        "selector": ".product-title"
      }
    }
  }
}
